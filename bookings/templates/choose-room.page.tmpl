{{template "base" .}}

{{define "content"}}
<div class="container">
  <div class="row">
    <div class="col">
      <p>Choose a room</p>

      {{$rooms := index .Data "rooms"}}

      <ul>
        {{range $rooms}}
        <li>
          <a href="/choose-room/{{.ID}}/{{.RoomName}}">{{.RoomName}}</a>
        </li>
        {{
          end
        }}
      </ul>
    </div>
  </div>
</div>
{{ end }}
